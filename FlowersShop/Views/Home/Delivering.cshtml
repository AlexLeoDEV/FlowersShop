
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>We are working in several cities and not going to stop!</h2>
<h3>Checkout all list.</h3>
<table class="table">
    <tr>
        <td><p>Title</p></td>
        <td><p>Price</p></td>
    </tr>

@foreach (var c in ViewBag.City)
{
    <tr>
        <td><p>@c.Title</p></td>
        <td><p>@c.Price</p></td>
    </tr>
}
</table>
